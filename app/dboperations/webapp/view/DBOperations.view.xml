
<mvc:View
    controllerName="dboperations.controller.DBOperations"
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:sap.m="sap.m"
    xmlns:f="sap.f"
    xmlns:w="sap.ui.integration.widgets"
    xmlns:tnt="sap.tnt"
    xmlns:form="sap.ui.layout.form"
    xmlns:table="sap.ui.table"
    xmlns:layout="sap.ui.layout"
    xmlns:core="sap.ui.core"
    xmlns:m="sap.m"
    xmlns:u="sap.ui.unified"
    xmlns:macros="sap.fe.macros"
>
    <Page
        id="_IDGenPage3"
        title="Real Estate App "
    >
        <content>
            <HBox id="_IDGenHBox3">
            <GenericTile
                    id="_IDGenGenericTile3"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Projects"
                    press="onNavigateToProjects"
                >
                    <TileContent
                        id="_IDGenTileContent2"
                        footer="Projects CRUD OPs"
                    >
                        <ImageContent
                            id="_IDGenImageContent2"
                            src="sap-icon://workflow-tasks"
                        />
                    </TileContent>
                </GenericTile>
                <GenericTile
                    id="_IDGenGenericTile1"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Buildings"
                    press="onNavigateToBuildings"
                >
                    <TileContent
                        id="_IDGenTileContent"
                        footer="Buildings CRUD OPs"
                    >
                        <ImageContent
                            id="_IDGenImageContent"
                            src="sap-icon://building"
                        />
                    </TileContent>
                </GenericTile>
                
                <GenericTile
                    id="_IDGenGenericTile2"
                    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
                    header="Units"
                    press="onNavigateToUnits"
                >
                    <TileContent
                        id="_IDGenTileContent1"
                        footer=" Units CRUD OPs"
                    >
                        <ImageContent
                            id="_IDGenImageContent1"
                            src="sap-icon://home"
                        />
                    </TileContent>
                </GenericTile>
                <GenericTile
    id="_IDGenGenericTile4"
    class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout"
    header="Payment Plans"
    press="onNavigateToPaymentPlans"
>
    <TileContent
        id="_IDGenTileContent4"
        footer="Payment Plans CRUD OPs"
    >
        <ImageContent
            id="_IDGenImageContent4"
            src="sap-icon://money-bills"
        />
    </TileContent>
</GenericTile>
            </HBox>
        </content>
    </Page>
</mvc:View>
